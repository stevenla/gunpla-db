{
	"name": "gunpla-scrape",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"deploy": "npm run build && npx gh-pages -d build --nojekyll --cname gunpla.fyi",
		"format": "prettier --write .",
		"fetch": "tsx ./scripts/scrape.ts && tsx ./scripts/parse.ts && cp ./scripts/cache/products.json ./src/lib/data/products.json",
		"clean-cache": "rm -rf ./scripts/cache ./static/images/*"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@types/eslint": "^8.56.0",
		"@types/jsdom": "^21.1.6",
		"@types/lodash": "^4.17.20",
		"@types/node-fetch": "^2.6.11",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"es-main": "^1.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0-next.4",
		"gh-pages": "^6.1.1",
		"jsdom": "^24.1.0",
		"lodash": "^4.17.21",
		"mkdirp": "^3.0.1",
		"moment": "^2.30.1",
		"node-fetch": "^3.3.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"promise-parallel-throttle": "^3.5.0",
		"sass": "^1.77.2",
		"svelte": "^5.34.7",
		"svelte-check": "^3.8.6",
		"svelte-virtual": "^1.0.0-next.22",
		"tslib": "^2.4.1",
		"tsx": "^4.20.3",
		"typescript": "^5.0.0",
		"virtua": "^0.41.5",
		"vite": "^5.0.3"
	},
	"type": "module"
}
